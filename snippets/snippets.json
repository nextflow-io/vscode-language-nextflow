{
    "shebang": {
        "prefix": ["#!", "shebang", "sh"],
        "body": "#!/usr/bin/env nextflow\n\n",
        "description": "Script shebang declaration"
    },
    "dsl2": {
        "prefix": "dsl2",
        "body": ["nextflow.enable.dsl=2\n","workflow $1{", "  $2", "}"],
        "description": "DSL 2 activation"
    },
    "process": {
        "prefix": "proc",
        "body": [
            "process $1 {",
            "  input:",
            "  $2",
            "  output:",
            "  $3",
            "  script:",
            "  \"\"\"",
            "  $4",
            "  \"\"\"",
            "}"
        ],
        "description": "Process definition"
    },
    "function": {
        "prefix": "def",
        "body": ["def $1($2) {", "  $3", "}"],
        "description": "Function definition"
    },
    "errorStrategy": {
        "prefix": "errorStrategy",
        "body": "errorStrategy '$1'",
        "description": "Process error strategy directive"
    },
    "clusterOptions": {
        "prefix": "clusterOptions",
        "body": "clusterOptions '$1'",
        "description": "Process cluster options directive"
    },
    "container": {
        "prefix": "container",
        "body": "container '$1'",
        "description": "Process container directive"
    },
    "containerOptions": {
        "prefix": "containerOptions",
        "body": "containerOptions '$1'",
        "description": "Process container options directive"
    },
    "cpus": {
        "prefix": "cpus",
        "body": "cpus $1",
        "description": "Process cpus directive"
    },
    "maxErrors": {
        "prefix": "maxErrors",
        "body": "maxErrors $1",
        "description": "Process max errors directive"
    },
    "maxRetries": {
        "prefix": "maxRetries",
        "body": "maxRetries $1",
        "description": "Process max retries directive"
    },
    "memory": {
        "prefix": "memory",
        "body": "memory '$1'",
        "description": "Process memory directive"
    },
    "publishDir": {
        "prefix": "publishDir",
        "body": "publishDir \"$1\", mode: '${2|symlink,copy,move,relink,link,copyNoFollow|}'",
        "description": "Process publish directory directive"
    },
    "tag": {
        "prefix": "tag",
        "body": "tag \"$1\"",
        "description": "Process tag directive"
    },
    "params": {
        "prefix": "params",
        "body": "params",
        "description": "Dictionary like object holding workflow parameters"
    },
    "launchDir": {
        "prefix": "launchDir",
        "body": "launchDir",
        "description": "The directory where the workflow is run"
    },
    "moduleDir": {
        "prefix": "moduleDir",
        "body": "moduleDir",
        "description": "The directory where a module script is located"
    },
    "projectDir": {
        "prefix": "projectDir",
        "body": "projectDir",
        "description": "The directory where the main script is located"
    },
    "workDir": {
        "prefix": "workDir",
        "body": "workDir",
        "description": "The directory where tasks temporary files are created"
    },
    "nextflow": {
        "prefix": "nextflow",
        "body": "nextflow",
        "description": "Dictionary like object representing nextflow runtime information"
    }
}